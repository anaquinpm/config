[user]
    name = My Name
    email = myMail@gmail.com
[core]
    editor = vim
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
    conflictstyle = diff3
[help]
    autocorrect = 20
[alias]
    st = status -sb
    cm = commit -m
    cma = commit -am
    cmne = commit --amend --no-edit
    dv = difftool -t vimdiff -y
    se = !git rev-list --all | xargs git grep -F
    ll = log --oneline --graph --decorate --all
    los = log --oneline --graph --decorate --all --stat
    last = log -1 HEAD --stat
    mr = push -u origin HEAD
[init]
    defaultBranch = main
